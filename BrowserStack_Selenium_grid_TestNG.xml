<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Sauce Las Test automtion With Selenium Grid"
       thread-count="3" parallel="tests" verbose="3">


    <test name="Sauce Test - FF">
        <parameter name="browserName" value="firefox" />
        <parameter name="browserversion" value="79.0" />
        <parameter name="platform" value="windows" />
        <parameter name="os_version" value="">10</parameter>

        <classes>
            <class name="BrowserStackSeleniumGrid" />
        </classes>

    </test>

    <test name="Sauce Test - Chrome">
        <parameter name="browserName" value="chrome" />
        <parameter name="browserversion" value="85.0" />
        <parameter name="platform" value="macOS" />
        <parameter name="os_version" value="">10.15</parameter>

        <classes>
            <class name="BrowserStackSeleniumGrid" />
        </classes>

    </test>

</suite>